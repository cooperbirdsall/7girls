{
  "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "ES2015",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "jsx": "react-jsx"
  },
  "lib": [
      "es2015"
  ]
}

